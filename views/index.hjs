<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    
  </head>
  <body>
    <div className="game">
        <div class="game-board">
          <div class="status"> </div>
          <div class="board-row"> 
            <button class="square" data-position="0"></button>
            <button class="square" data-position="1"></button>
            <button class="square" data-position="2"></button>
          </div>
          <div class="board-row"> 
            <button class="square" data-position="3"></button>
            <button class="square" data-position="4"></button>
            <button class="square" data-position="5"></button> 
          </div>
          <div class="board-row"> 
            <button class="square" data-position="6"></button>
            <button class="square" data-position="7"></button>
            <button class="square" data-position="8"></button>
          </div>
        </div>
        <div class="game-info">
          <div></div>
          <ol></ol>
        </div>
    </div>
    
    
     <script type="text/javascript" src="/javascripts/game-logic.js"></script>
  </body>
</html>
